import java.util.Scanner;
class A{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        try{
            int a=sc.nextInt();
            int b=sc.nextInt();
            System.out.println(a/b);
            
        }
        catch(ArithmeticException e)
        {
            System.out.println("div by 0");
        }
        finally{
            System.out.println("hi");
        }
    }
}
output
7 7
1
hi
